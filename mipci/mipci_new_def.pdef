<ccm_net_get                            type="ccm_net_get"                 alias="CcmGetNetworkInfoRequest">
    <sess                               type="ccm_session"/>
    <tokens                             type="lenstr"   counts="0-10"   />
    <items                              type="lenstr"   counts="0-10"  info="all|phy|phy.conf|phy.info|ipv4|wifi_client|wifi_adhoc|pppoe|default_gw"/>
    <force_scan                         type="int32"                    />
</ccm_net_get>

<ccm_net_get_ack                        type="ccm_net_get_ack"             alias="CcmGetNetworkInfoResponse">
    <ret                                type="ccm_ret"/>
    <info                               type="ccm_net_info_m"               alias="ccm_net_info">
        <ifs                            type="ccm_net_interface_m"                counts="1-100"                          info="List of network interfaces">
            <token                          type="lenstr"                           info="Unique identifier referencing the physical entity"/>
            <enabled                        type="uint32"                           info="Indicates whether or not an interface is enabled"/>
            <phy            type="ccm_phy_m"  counts="0-1"                            info="Network interface information">
                <conf       type="ccm_phy_conf"     counts="0-1">
                    <mode                   type="lenstr"                           inof="ether:ether, wifi:wificlient,adhoc"/>
                    <mtu                    type="uint32"                           info="Maximum transmission unit"/>
                </conf>
                <info       type="ccm_phy_info_m"     counts="0-1"    info="READONLY">
                    <name                   type="lenstr"                           info="Network interface name, for example eth0"/>
                    <type                   type="lenstr"                           info="ether,wifi,gprs,wcdma,lte, READONLY"/>
                    <mac                    type="lenstr"                           info="Network interface MAC address" />
                    <stat                   type="lenstr"                           info="ok;err;" />
                </info>
            </phy>
            <ipv4           type="ccm_ipv4_m"         counts="0-1"                            info="IPv4 network interface configuration">
                <conf       type="ccm_ipv4_conf_m"    counts="0-1">
                    <enabled                type="uint32"               info="Indicates whether or not IPv4 is enabled"/>
                    <mode                   type="lenstr"               info="static,dhcp"/>
                    <static_ip              type="ccm_ip_sets"          counts="0-1"    />
                    <debug_ip               type="ccm_ip_sets"          counts="0-1"    />
                </conf>
                <info       type="ccm_ipv4_info_m" counts="0-1"    info="READONLY">
                    <stat                   type="lenstr"               info="ok;err;err.invalid_param;err.ping_failure;err.gw_empty"/>
                    <ip                     type="ccm_ip_sets"          />
                </info>
            </ipv4>
            <wifi_client    type="ccm_wifi_client_m"  counts="0-1">
                <conf       type="ccm_wifi_client_conf"     counts="0-1">
                    <enabled                type="uint32"               />
                    <ssid                   type="lenstr"               />
                    <auth_mod               type="lenstr"               info="discard;"/>
                    <encrypt_mod            type="lenstr"               info="discard;"/>
                    <key                    type="lenstr"               />
                    <usr                    type="lenstr"               />
                </conf>
                <info       type="ccm_wifi_client_info_m"     counts="0-1"    info="READONLY">
                    <channel                type="int32"               />
                    <stat                   type="lenstr"              info="ok;err;err.invalid_param;info.connecting"/>
                </info>
                <ap_list    type="ccm_wifi_info"              counts="0-10" >
                    <ssid                           type="lenstr"           />
                    <connect                        type="int32"            />
                    <quality                        type="int32"            />

                    <bss                            type="lenstr"           info="mac"/>
                    <proto                          type="lenstr"           info="B/G/N"/>
                    <mode                           type="lenstr"           info="20HZ;50HZ"/>
                    <freq                           type="lenstr"           info="2.4G(like 2437);5G"/>
                    <signal                         type="int32"            info="signale level"/>
                    <noise                          type="int32"            info="noise level"/>
                    <bitrate                        type="lenstr"           />

                    <encrypt                        type="lenstr"           />
                    <adhoc                          type="int32"            />

                    <wps                counts="0-1">
                        <ver                        type="lenstr"   />
                        <mfc                        type="lenstr"   info="manfacuturer"/>
                        <model                      type="lenstr"   />
                        <dev_name                   type="lenstr"   />
                        <conf_methods               type="lenstr"   />
                    </wps>

                    <ext counts="0-1">
                        <tsf                            type="lenstr"           />
                        <beacon_interval                type="int32"            info="unit:ms"          />
                        <cap                            type="lenstr"           info="capability"       />
                        <last_seen                      type="int32"            info="unit:ms(ago); "   />
                        <supported_rates                type="lenstr"           />
                        <ds_parameter_set               type="int32"            info="unit:channel"     />
                        <country                        type="lenstr"           />
                        <country_data                   type="lenstr"           />
                        <erp                            type="lenstr"           />
                        <extended_supported_rates       type="lenstr"           />

                        <ht    counts="0-1">
                            <caps                           type="lenstr"           counts="0-20"       info="in iw source code, this print max 20 line"/>
                            <maximum_rx_ampdu_length        type="lenstr"           />
                            <minimum_rx_ampdu_time_spacing  type="lenstr"           />
                            <ht_rx_mcs_rate_indexes_supported   type="lenstr"       />
                            <ht_tx_mcs_rate_indexes             type="lenstr"       />
                        </ht>
                        <rsn                counts="0-1"    type="ccm_wifi_info_rsn">
                            <ver                            type="lenstr"           />
                            <group_cipher                   type="lenstr"           />
                            <pairwise_ciphers               type="lenstr"           />
                            <auth_suites                    type="lenstr"           />
                            <cap                            type="lenstr"           />
                        </rsn>
                        <wpa                counts="0-1"    type="ccm_wifi_info_rsn" />
                        <wmm                counts="0-1">
                            <param_ver                  type="lenstr"   />
                            <be                         type="lenstr"   />
                            <bk                         type="lenstr"   />
                            <vi                         type="lenstr"   />
                            <vo                         type="lenstr"   />
                        </wmm>
                    </ext>
                </ap_list>
            </wifi_client>
            <wifi_adhoc     type="ccm_wifi_adhoc_m"   counts="0-1">
                <conf       type="ccm_wifi_adhoc_conf"  counts="0-1">
                    <enabled                type="uint32"               />
                    <ssid                   type="lenstr"               />
                    <auth_mod               type="lenstr"               info="discard"/>
                    <encrypt_mod            type="lenstr"               info="discard"/>
                    <key                    type="lenstr"               info="length 8-63 bytes"/>
                </conf>
                <info       type="ccm_wifi_adhoc_info_m"  counts="0-1"    info="READONLY">
                    <stat                   type="lenstr"               info="discard"/>
                    <channel                type="lenstr"               />
                </info>
            </wifi_adhoc>
            <pppoe          type="ccm_pppoe_m"        counts="0-1"        info="discard;">
                <conf       type="ccm_pppoe_conf"   counts="0-1">
                    <enabled                type="uint32"               info="Indicates whether or not IPv4 is enabled"/>
                    <user                   type="lenstr"               />
                    <password               type="lenstr"               />
                </conf>
                <info       type="ccm_pppoe_info_m"   counts="0-1"        info="READONLY">
                    <stat                   type="lenstr"               info="discard"/>
                </info>
            </pppoe>
            <dhcp_srv       type="ccm_dhcp_srv_m" counts="0-1">
                <conf       type="ccm_dhcp_srv_conf" counts="0-1">
                    <enabled                type="uint32"               info="Indicates whether or not IPv4 is enabled"/>
                    <gw                     type="ip"                   info="gate way address!!!!!!"/>
                    <mask                   type="ip"                   info="discard;"/>
                    <ip_start               type="ip"                   info="IPv4 address"/>
                    <ip_end                 type="ip"                   info="IPv4 address"/>
                </conf>
                <info       type="ccm_dhcp_srv_info_m" counts="0-1" info="READONLY">
                    <stat                   type="lenstr"               info="discard;"/>
                </info>
            </dhcp_srv>
            <internet           type="ccm_internet_m"         counts="0-1">
                <conf           type="ccm_internet_conf"    counts="0-1">
                    <domain     type="lenstr"                           info="discard;"/>
                </conf>
                <info           type="ccm_internet_info_m"    counts="0-1">
                    <stat                   type="lenstr"               info="ok;err;err.ping_failure;err.domain_empty"   />
                </info>
            </internet>
        </ifs>
        <dns                type="ccm_dns_m"          counts="0-1">
            <conf           type="ccm_dns_conf"     counts="0-1">
                <enabled                    type="int32"                />
                <mode                       type="lenstr"               info="static,dhcp"/>
                <static_dns                 type="ip"                   counts="0-100" />
            </conf>
            <info           type="ccm_dns_info_m"     counts="0-1"                info="READONLY">
                <stat                       type="lenstr"               info="ok;" />
                <dns                        type="ip"                   counts="0-100"/>
            </info>
        </dns>
    </info>
</ccm_net_get_ack>

