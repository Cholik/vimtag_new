        </VideoSourceConfiguration>

        <AudioSourceConfiguration       type="ccm_mic_conf"                     counts="0-1"/>

        <VideoEncoderConfiguration      type="ccm_video_enc_conf"               alias="VideoEncoderConfiguration"       counts="0-1">
            <Entity                     type="ccm_conf_entity"/>
            <Encoding                   type="lenstr"                           info="Used video codec, either Jpeg, H.264 or Mpeg4"/>
            <Resolution                 type="ccm_video_resolution"             info="Configured video resolution"/>
            <Quality                    type="uint32"                           info="Relative value for the video quantizers and the quality of the video. A high value within supported quality range means higher quality"/>
            <RateControl                type="VideoRateControl">
                <type                   type="lenstr"                           info="cbr; vbr; "   />
                <FrameRateLimit         type="uint32"                           info="Maximum output framerate in fps. If an EncodingInterval is provided the resulting encoded framerate will be reduced by the given factor"/>
                <EncodingInterval       type="uint32"                           info="Interval at which images are encoded and transmitted. (A value of 1 means that every frame is encoded, a value of 2 means that every 2nd frame is encoded ...)"/>
                <BitrateLimit           type="uint32"                           info="the maximum output bitrate"   />
                <BitrateMin             type="uint32"                           info="the minimum output bitrate"   />
                <BitrateInit            type="uint32"                           info="the init ouput bitrate"       />
            </RateControl>
            <H264                       type="H264Configuration"                counts="0-1">
                <GovLength              type="uint32"                           info="Group of Video frames length. Determines typically the interval in which the I-Frames will be coded. An entry of 1 indicates I-Frames are continuously generated. An entry of 2 indicates that every 2nd image is an I-Frame, and 3 only every 3rd frame, etc. The frames in between are coded as P or B Frames"/>
                <H264Profile            type="lenstr"                           info="the H.264 profile, either baseline, main, extended or high"/>
            </H264>
            <enc_chn                    type="int32"                            info="readonly"/>
        </VideoEncoderConfiguration>

        <AudioEncoderConfiguration      type="ccm_audio_enc_conf"               alias="AudioEncoderConfiguration"   counts="0-1">
            <Entity                     type="ccm_conf_entity"/>
            <Encoding                   type="lenstr"                           info="Audio codec used for encoding the audio input (either G.711, G.726, AMR, ADPCM or AAC)"/>
            <Bitrate                    type="uint32"                           info="The output bitrate in kbps."/>
            <SampleRate                 type="uint32"                           info="The output sample rate in kHz."/>
        </AudioEncoderConfiguration>
        <token                          type="lenstr"           />
        <fixed                          type="int32"            />
    </profile>
